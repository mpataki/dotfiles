alias e='vim'

# git
alias g='git'
alias gadd='git add -u && git add . && git s'
alias gs='git s'
alias gap='git ap'
alias ga='git a'
alias gd='git d'
alias gc='git c'
alias gpl='git pl'
alias gps='git ps'

# ruby
alias be='bundle exec'

# system
alias ls='ls -G' # turn on colors

# experimental
alias notesync='aws s3 sync s3://note-store ~/notes && aws s3 sync ~/notes s3://note-store'
